@model e_CarSharing.Models.Type

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Eliminar</h2>

<h3>Deseja eliminar este item?</h3>
<div>
    <hr />
    @if (ViewBag.ShowAlert != null && ViewBag.ShowAlert == true)
    {
        @Html.Partial("_ShowAlert");
    }
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.TypeName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TypeName)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

    <div class="form-actions no-color">
        <button type="button" class="btn btn-secondary btn-sm" onclick="history.go(-1); return false;" href="#">
            <span class="glyphicon glyphicon-chevron-left"></span>&nbsp;Voltar
        </button>
        <button type="submit" value="Eliminar" class="btn btn-danger btn-sm">
            Eliminar
        </button>
    </div>
    }
</div>
